{
  "name": "indebrau-backend",
  "version": "0.0.1",
  "description": "Provides the backend for indebrau",
  "repository": "https://github.com/PedeLa/indebrau/",
  "scripts": {
    "start": "nodemon -e js,graphql -x node -r dotenv/config src/index.js",
    "debug": "nodemon -e js,graphql -x node --inspect -r dotenv/config src/index.js",
    "playground": "graphql playground",
    "dev": "npm-run-all --parallel start playground"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.4.0",
    "prisma-binding": "^2.3.2",
    "cookie-parser": "^1.4.4"
  },
  "devDependencies": {
    "dotenv": "^6.2.0",
    "graphql-cli": "^3.0.9",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "prisma": "^1.26.4"
  },
  "author": "Peter de Lange",
  "license": "ISC"
}
