{
  "name": "indebrau-backend",
  "version": "0.1.0",
  "private": true,
  "description": "Provides a backend for indebrau",
  "repository": "https://github.com/PedeLa/indebrau-backend",
  "scripts": {
    "deploy": "npm run prisma && npm run start",
    "prisma": "npx prisma introspect && npx prisma generate",
    "start": "node -r dotenv/config src/index.js",
    "dev": "nodemon -e js,graphql -x node --inspect -r dotenv/config src/index.js"
  },
  "engines": {
    "node": "~12.19.0",
    "npm": "~6.14.8"
  },
  "dependencies": {
    "@prisma/client": "2.9.0",
    "apollo-server-express": "2.18.2",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.5",
    "dotenv": "8.2.0",
    "fs-extra": "9.0.1",
    "jsonwebtoken": "8.5.1",
    "multer": "1.4.2"
  },
  "devDependencies": {
    "@prisma/cli": "2.9.0",
    "eslint": "7.11.0",
    "nodemon": "2.0.5"
  },
  "author": "Peter de Lange",
  "license": "ISC"
}
